{
  "manifest_version": 2,

  "name": "cocoProxy",
  "description": "an simply and powerful Proxy Control Manager",
  "version": "1.2",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "bookmarks",
    "proxy",
    "webRequest",
    "storage"
  ],
  "content_scripts": [ {  
          "exclude_globs": [  ],  
          "include_globs": [ "*" ],  
          "js": [ "content_script.js" ,"js/jquery-3.1.1.js"],  
          "matches": [ "http://*/*", "https://*/*" ],  
          "run_at": "document_end"  
   } ], 
   "web_accessible_resources": [
      "includeme.js",
      "popup.js",
      "css/content_css.css"
    ],
    "options_page": "option.html"
}